{
  "version": 2,
  "name": "OnLine",
  "builds": [
    {
      "src": "nest build", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "dist/main.js", 
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "public/$1" 
    }
  ]
}

  
