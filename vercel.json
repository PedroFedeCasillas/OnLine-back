{
    "version": 2,
    "name": "OnLine",
    "builds": [
      {
        "src": "dist/src/main.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/src/main.js", 
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "public/$1" 
      }
    ]
  }
  
