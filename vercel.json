{
  "version": 2,
  "name": "OnLine",
  "builds": [
    {
      "src": "dist/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/docs",
      "dest": "dist/main.js"
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}


